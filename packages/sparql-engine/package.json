{
  "name": "@jahed/sparql-engine",
  "version": "0.9.0",
  "description": "SPARQL query engine for servers and web browsers.",
  "author": "Jahed Ahmed",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jahed/sparql-engine.git",
    "directory": "packages/sparql-engine"
  },
  "bugs": "https://github.com/jahed/sparql-engine/issues",
  "funding": "https://jahed.dev/donate",
  "keywords": [
    "sparql",
    "engine",
    "query",
    "graph",
    "database",
    "rdf",
    "turtle",
    "semantic",
    "semantic web"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./*.ts": {
      "default": "./src/*.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "AUTHORS"
  ],
  "scripts": {
    "clean": "rm -rf dist LICENSE AUTHORS",
    "build": "npm run clean && tsc -p tsconfig.build.json && cp ../../LICENSE ../../AUTHORS .",
    "format": "prettier -w .",
    "lint": "tsc --noEmit",
    "test": "npm run test:file -- 'tests/**/*-test.ts'",
    "test:file": "node --disable-warning=ExperimentalWarning --experimental-strip-types --experimental-test-isolation=none --test-timeout=5000 --test"
  },
  "dependencies": {
    "@seald-io/binary-search-tree": "^1.0.3",
    "date-fns": "^4.1.0",
    "js-md5": "^0.8.3",
    "lodash-es": "^4.17.21",
    "lru-cache": "^11.1.0",
    "rdf-data-factory": "^2.0.2",
    "rdf-string": "^2.0.1",
    "rxjs": "^8.0.0-alpha.14",
    "sparqljs": "^3.7.3"
  },
  "devDependencies": {
    "@jahed/sparql-engine-tests": "^0.0.1"
  },
  "overrides": {
    "sparqljs": {
      "rdf-data-factory": "$rdf-data-factory"
    }
  }
}
